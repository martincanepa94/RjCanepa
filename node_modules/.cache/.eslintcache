) => {\n                if (catId) {\n\n                    setProductos(res.filter((el) => el.categoria === catId ))\n\n                    \n                } else {\n                    setProductos(res)\n                }\n                \n            })\n            .catch((err) => {\n                console.log(err)\n            })\n            .finally(() => {\n                setLoading(false)\n            })\n\n\n    }, [catId])\n\n    return (\n            <>\n            {\n                loading ? \n                <Loader />\n                : <ItemList productos={productos}/>\n            }\n                \n            </>\n\n    )\n}","/Users/martincanepa/Dropbox/Facultad/CoderHouse/ReactJs/proyecto-martinc/src/components/NavBar/NavBar.js",["100","101"],"import \"./NavBar.css\"\nimport {Link} from 'react-router-dom'\nimport { CartWidget } from \"../CartWidget/CartWidget\"\nimport { Card, Button, Navbar, Nav, Container } from \"react-bootstrap\"\n\nexport const NavBar = () => {\n\n    return (\n        <>\n  <Navbar bg=\"primary\" variant=\"dark\">\n    <Container>\n    <Navbar.Brand as={Link} to=\"/\"><h1>HOME LOGO</h1></Navbar.Brand>\n\n    <Nav className=\"me-auto\">\n      <Nav.Link as={Link} to='/productos/estandar'>Productos Estandar</Nav.Link>\n      <Nav.Link as={Link} to='/productos/vip'>Productos VIP</Nav.Link>\n      <Nav.Link as={Link} to='/contacto'>Contacto</Nav.Link>\n      <Nav.Link as={Link} to='/nosotros'>Nosotros</Nav.Link>\n      \n    </Nav>\n    <CartWidget/>\n    </Container>\n  </Navbar>\n      \n        </>    \n    )\n}","/Users/martincanepa/Dropbox/Facultad/CoderHouse/ReactJs/proyecto-martinc/src/components/Boton/Boton.js",[],"/Users/martincanepa/Dropbox/Facultad/CoderHouse/ReactJs/proyecto-martinc/src/components/CartWidget/CartWidget.js",["102"],"export const CartWidget = () => {\n    return (\n            <>\n            <img src=\"https://cdn-icons-png.flaticon.com/512/102/102657.png\" \n            width=\"60\" height