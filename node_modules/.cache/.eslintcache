ailContainer.js",["130"],"import React, { useEffect, useState } from 'react';\nimport { pedirDatos } from '../../helpers/pedirDatos';\nimport { useParams } from 'react-router-dom';\nimport { ItemDetail } from '../ItemDetail/ItemDetail';\n\nexport const ItemDetailContainer = () => {\n\n    const [loading, setLoading] = useState(false)\n    const [item, setItem] = useState(null)\n\n    const { itemId } = useParams()\n\n    console.log(itemId)\n    console.log(item)\n\n    useEffect( () => {\n        setLoading(true)\n\n        pedirDatos()\n            .then((res) => {\n                setItem( res.find((el) => el.id === Number(itemId) ) )\n            })\n            .finally(() => {\n                setLoading(false)\n            })\n\n    }, [])\n\n    useEffect(()=> {\n        const clickear = () => {\n            console.log('click')\n        }\n\n\n        window.addEventListener('click', clickear)\n\n        return () => {\n           window.removeEventListener('click', clickear)\n        }\n\n    }, [])\n\n    return (\n    <div>\n        {\n            loading\n            ? <h2>Cargando...</h2>\n            : <ItemDetail {...item}/>\n        }\n\n    </div>\n)\n}","/Users/martincanepa/Dropbox/Facultad/CoderHouse/ReactJs/proyecto-martinc/src/components/ItemDetail/ItemDetail.js",["131"],"\nimport { ItemCount } from \"../ItemCount/ItemCount\"\nimport { useEffect, useState } from \"react\"\n\nexport const ItemDetail = ( { id, nombre, img, desc, stock, precio, categoria } ) => {\n\n    const [cantidad, setCantidad] = useState(0)\n\n    const handleAgregar = () => {\n        if (cantidad === 0) return\n        // con el return ahi no se ejecuta lo que sigue en la funcion.\n\n\n        const addItem = {\n            id, nombre, precio, stock, cantidad\n        }\n\n        console.log(addItem)\n    }\n\n    return (\n        <div>\n            <h3>{nombre}</h3>\n            <img src={img} alt={nombre}/>\n            <p>{desc}</p>\n            <h5>Precio $: {precio}</h5>\n            \n            <ItemCount \n            max={stock} \n            counter={cantidad} \n            setCounter={setCantidad} \n            />\n\n\n            <button\n                className=\"btn btn-success my-2\"\n                onClick={handleAgregar}\n            \n            >\n                Agregar al carrito\n                </button>\n        \n\n        </div>\n    )\n}","/Users/martincanepa/Dropbox/Facultad/CoderHouse/ReactJs/proyecto-martinc/src/components/ItemCount/ItemCount.js",["132","133"],"import { useEffect, useState } from \"react\"\n\nexport const ItemCount = ({max, min = 0, counter, setCounter}) => {\n\n    const handleSumar = () => {\n  \n        counter < max && setCounter(counter+1)\n    }\n\n   const handleRestar = () => {\n       counter > min && setCounter(counter-1)\n       // Esto es un ternario de dos partes. si pasa tal cosa, entonces esto. mas nada.\n   }\n\n    \n\n    return (\n        <div >\n            <button className=\"btn btn-outline-primary\" onClick={handleRestar}>-</button>\n            <span className=\"mx-3\">{counter}</span>\n            <button className=\"btn btn-outline-primary\" onClick={handleSumar}> +</button>\n        </div>\n\n\n    )\n}","/Users/martincanepa/Dropbox/Facultad/CoderHouse/ReactJs/proyecto-martinc/src/components/Form/Form.js",[],{"ruleId":"134","replacedBy":"135"},{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","severity":1,"message":"139","line":7,"column":10,"nodeType":"140","messageId":"141","endLine":7,"endColumn":15},{"ruleId":"138","severity":1,"message":"142","line":10,"column":10,"nodeType":"140","messageId":"141","endLine":10,"endColumn":14},{"ruleId":"134","replacedBy":"143"},{"ruleId":"136","replacedBy":"144"},{"ruleId":"138","severity":1,"message":"139","line":10,"column":10,"nodeType":"140","messageId":"141","endLine":10,"endColumn":15},{"ruleId":"138","severity":1,"message":"145","line":11,"column":10,"nodeType":"140","messageId":"141","endLine":11,"endColu